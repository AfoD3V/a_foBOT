{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "DOCKERFILE",
    "buildCommand": "docker build -t a_fobot ."
  },
  "deploy": {
    "startCommand": "run docker -e DISCORD_TOKEN=$DISCORD_TOKEN -e SECRET_TOKEN=$TOKEN_SECRET a_fobot",
    "healthcheckPath": "/",
    "healthcheckTimeout": 100,
    "restartPolicyType": "never"
  }
}